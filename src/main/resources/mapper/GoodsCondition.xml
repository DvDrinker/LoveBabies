    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lanou.dao.GoodsConditionMapper">

	<resultMap type="GoodsCondition" id="GoodsConditionResult">
		<id column="ConditionId" property="conditionId" javaType="integer" />
